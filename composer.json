{
    "name": "metamodels/dropzone_file_upload",
    "description": "MetaModels dropzone for the attribute file",
    "keywords": [
        "contao",
        "metamodels",
        "attribute",
        "file",
        "dropzone",
        "upload"
    ],
    "type": "contao-bundle",
    "homepage": "http://now.metamodel.me/",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        },
        {
            "name": "Sven Baumann",
            "email": "baumann.sv@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "mail@metamodel.me",
        "issues": "https://github.com/MetaModels/dropzone_file_upload/issues",
        "wiki": "http://de.contaowiki.org/MetaModels",
        "irc": "irc://irc.freenode.org/contao.mm",
        "source": "https://github.com/MetaModels/dropzone_file_upload"
    },
    "require": {
        "php": "^7.2",
        "contao-community-alliance/dc-general": "^2.2@dev",
        "contao/core-bundle": "^4.9.0, <4.13.0",
        "contao-components/dropzone": "^5.0.1",
        "metamodels/core": "^2.2@dev",
        "metamodels/attribute_file": "^2.2@dev",
        "symfony/dependency-injection": "4.4.*",
        "symfony/http-kernel": "4.4.*",
        "ext-pdo": "*"
    },
    "require-dev": {
        "contao/manager-plugin": "^2.8",
        "metamodels/contao-frontend-editing": "^2.2@dev",
        "contao/manager-bundle": "^4.9.0, <4.13.0",
        "phpcq/all-tasks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "MetaModels\\DropzoneFileUploadBundle\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MetaModels\\DropzoneFileUploadBundle\\Test\\": "tests"
        }
    },
    "extra": {
        "contao-manager-plugin": "MetaModels\\DropzoneFileUploadBundle\\ContaoManager\\Plugin",
        "branch-alias": {
            "dev-release/2.2.0": "2.2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
